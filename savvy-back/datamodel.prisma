enum Permission {
  ADMIN
  USER
  CASECREATE
  CASEUPDATE
  CASEDELETE
  PERMISSIONUPDATE
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  resetToken: String
  resetTokenExpiry: Float
  permissions: [Permission]
}

type Case {
  id: ID! @unique
  title: String!
  description: String!
  mainText: String
  issue: String
  rule: String
  analysis: String
  conclusion: String
  image: String!
  largeImage: String!
  price: Int!
  user: User!
}